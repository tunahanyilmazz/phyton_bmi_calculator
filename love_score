# ðŸš¨ Don't change the code below ðŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ðŸš¨ Don't change the code above ðŸ‘†

#Write your code below this line ðŸ‘‡

l_name1 = name1.lower()
l_name2 = name2.lower()
add_name = l_name1+l_name2

t_count = add_name.count("t")
r_count = add_name.count("r")
u_count = add_name.count("u")
e_count = add_name.count("e")
l_count = add_name.count("l")
o_count = add_name.count("o")
v_count = add_name.count("v")

true_total = t_count + r_count + u_count + e_count
love_total = l_count +  o_count + v_count + e_count
love_score = str(true_total) + str(love_total)
num = int(love_score)

if (num < 10) or (num > 90):
    print(f"Your score is {love_score}, you go together like coke and mentos.")
elif (num >= 40) and (num <= 50):
    print(f"Your score is {love_score}, you are alright together.")
else:
    print(f"Your score is {love_score}")





